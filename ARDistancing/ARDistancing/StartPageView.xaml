<?xml version="1.0" encoding="utf-8" ?>
<views:BasePageView
    x:Class="ARDistancing.StartPageView"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:arDistancing="clr-namespace:ARDistancing;assembly=ARDistancing"
    xmlns:views="clr-namespace:ARDistancing.Views;assembly=ARDistancing"
    x:DataType="arDistancing:StartPageViewModel"
    x:TypeArguments="arDistancing:StartPageViewModel">
    <Button
        Margin="20"
        BackgroundColor="Aquamarine"
        Command="{Binding GotoArPageCommand}"
        Text="Goto AR Page"
        TextColor="White"
        VerticalOptions="End" />
</views:BasePageView>